{
    "manifest_version": 3,
    "name": "search-track",
    "version": "1.0.0",
    "action" : {
        "default_popup": "popup.html",
        "default_title": "Search Track"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "storage",
        "identity",
        "webNavigation",
        "storage",
        "activeTab",
        "identity.email",
        "cookies"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "http://localhost/*",
        "https://search-track-frontend.vercel.app/*"
    ]    
}